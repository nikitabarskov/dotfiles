{
	"agent": {
		"commit_message_model": {
			"model": "claude-haiku-4.5",
			"provider": "copilot_chat"
		},
		"default_model": {
			"model": "claude-sonnet-4.5",
			"provider": "copilot_chat"
		},
		"default_profile": "ask",
		"dock": "left",
		"enable_feedback": false,
		"inline_assistant_model": {
			"model": "claude-sonnet-4.5",
			"provider": "copilot_chat"
		},
		"thread_summary_model": {
			"model": "claude-haiku-4.5",
			"provider": "copilot_chat"
		}
	},
	"agent_buffer_font_size": 12.0,
	"agent_ui_font_size": 12.0,
	"audio": {
		"experimental.denoise": true,
		"experimental.rodio_audio": true
	},
	"auto_install_extensions": {
		"biome": true,
		"tofu": true
	},
	"auto_signature_help": true,
	"autosave": "on_focus_change",
	"bottom_dock_layout": "full",
	"buffer_font_family": "IosevkaTerm Nerd Font Propo",
	"buffer_font_size": 12,
	"buffer_line_height": "comfortable",
	"calls": {
		"mute_on_join": true
	},
	"drag_and_drop_selection": {
		"delay": 300
	},
	"edit_predictions": {},
	"file_scan_inclusions": [".env*", "NOTES", "*.diff"],
	"file_types": {
		"OpenTofu": ["tofu"],
		"OpenTofu Vars": ["tfvars"]
	},
	"format_on_save": "off",
	"git": {
		"inline_blame": {
			"show_commit_summary": true
		}
	},
	"hover_popover_delay": 300,
	"minimap": {},
	"multi_cursor_modifier": "cmd_or_ctrl",
	"outline_panel": {
		"dock": "right"
	},
	"project_panel": {
		"auto_fold_dirs": false,
		"auto_open": { "on_paste": false },
		"dock": "right",
		"entry_spacing": "standard",
		"file_icons": true,
		"hide_root": true,
		"indent_size": 10.0,
		"show_diagnostics": "errors"
	},
	"relative_line_numbers": "enabled",
	"restore_on_startup": "none",
	"search": {
		"button": true
	},
	"show_edit_predictions": false,
	"soft_wrap": "preferred_line_length",
	"status_bar": {
		"cursor_position_button": true
	},
	"telemetry": {
		"diagnostics": false,
		"metrics": false
	},
	"terminal": {
		"dock": "bottom"
	},
	"theme": {
		"dark": "Catppuccin Mocha (sky) - No Italics",
		"light": "Catppuccin Latte (sky) - No Italics",
		"mode": "system"
	},
	"title_bar": {
		"show_branch_icon": true,
		"show_menus": false,
		"show_onboarding_banner": true
	},
	"ui_font_family": "IosevkaTerm Nerd Font Propo",
	"ui_font_size": 12,
	"ui_font_weight": 400.0,
	"wrap_guides": [80, 120]
}
