export PYENV_ROOT=$HOME/.pyenv
export POETRY_HOME=$HOME/.poetry
export TOOLS_HOME=$HOME/tools
export PROTOC_HOME=$TOOLS_HOME/protoc
export JETBRAINS_HOME=$TOOLS_HOME/jetbrains
export BZIP2_HOME=/usr/local/opt/bzip2
export CARGO_HOME=$HOME/.cargo
export PATH=$BZIP2_HOME/bin:$POETRY_HOME/bin:$PYENV_ROOT/shims:$PYENV_ROOT/bin:$PROTOC_HOME/include:$PROTOC_HOME/bin:$CARGO_HOME/bin:$JETBRAINS_HOME:$PATH
export STARSHIP_CONFIG=$HOME/.starship.toml


export PATH="$PATH:/usr/local/sbin"
export LDFLAGS="-L/usr/local/opt/zlib/lib -L/usr/local/opt/bzip2/lib"
export CPPFLAGS="-I/usr/local/opt/zlib/include -I/usr/local/opt/bzip2/include"

export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion

typeset -U PATH
